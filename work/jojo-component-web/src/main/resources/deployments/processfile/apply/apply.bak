          <activiti:field name="approvedRequired">
            <activiti:string><![CDATA[yes]]></activiti:string>
          </activiti:field>
        </activiti:taskListener>
      </extensionElements>
    </userTask>
    <userTask id="usertask3" name="物资主管询价并确认"></userTask>
