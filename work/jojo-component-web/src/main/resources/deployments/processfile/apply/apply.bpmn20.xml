	<process id="materialApplyProcess" name="物料申请流程" isExecutable="true">
		<userTask id="userTaskGLMgrcheck" name="管理主管审核" activiti:assignee="${nextAssignee}">
			<documentation>管理主管 审核轮机长提交的物料申请</documentation>
		</userTask>
		<sequenceFlow id="endFlow" sourceRef="userTaskGLMgrcheck" targetRef="usertask1"></sequenceFlow>
		<endEvent id="theEnd" name="流程结束">
			<documentation>流程结束</documentation>
			<extensionElements>
				<activiti:taskListener event="complete"
					class="com.jojo.service.process.usertask.listener.TaskFinishInstHandler">
					<activiti:field name="status">
						<activiti:string><![CDATA[9]]></activiti:string>
					</activiti:field>
				</activiti:taskListener>
			</extensionElements>
		</endEvent>
		<userTask id="usertask1" name="管理领导审核" activiti:assignee="${nextAssignee}"></userTask>
		<userTask id="usertask2" name="安技主管审核" activiti:assignee="${nextAssignee}">
			<documentation>安技主管审核物料信息</documentation>
			<extensionElements>
				<activiti:taskListener event="create"
					class="com.jojo.service.process.usertask.listener.TaskAssignmentHandler">
					<activiti:field name="approvedRequired">
						<activiti:string><![CDATA[yes]]></activiti:string>
					</activiti:field>
				</activiti:taskListener>
			</extensionElements>
		</userTask>
		<userTask id="usertask3" name="物资主管询价并确认" activiti:assignee="${nextAssignee}"></userTask>
		<sequenceFlow id="flow1" sourceRef="usertask1" targetRef="usertask2"></sequenceFlow>
		<sequenceFlow id="flow2" sourceRef="usertask2" targetRef="exclusivegateway1"></sequenceFlow>
		<sequenceFlow id="flow3" name="通过" sourceRef="exclusivegateway1" targetRef="usertask3">
			<conditionExpression xsi:type="tFormalExpression"><![CDATA[${approvedByManager == 1}]]></conditionExpression>
		</sequenceFlow>
		<sequenceFlow id="flow4" name="打回" sourceRef="exclusivegateway1" targetRef="userTaskGLMgrcheck">
			<conditionExpression xsi:type="tFormalExpression"><![CDATA[${approvedByManager == 2}]]></conditionExpression>
		</sequenceFlow>
		<userTask id="usertask4" name="安技主管确认" activiti:assignee="${nextAssignee}"></userTask>
		<userTask id="usertask5" name="物资经理确认" activiti:assignee="${nextAssignee}"></userTask>
		<sequenceFlow id="flow5" sourceRef="usertask3" targetRef="usertask4"></sequenceFlow>
		<sequenceFlow id="flow6" sourceRef="usertask4" targetRef="usertask5"></sequenceFlow>
		<userTask id="usertask6" name="管理主管确认并录入" activiti:assignee="${nextAssignee}"></userTask>
		<sequenceFlow id="flow7" sourceRef="usertask5" targetRef="usertask6"></sequenceFlow>
		<userTask id="usertask7" name="轮机长录入备件" activiti:assignee="${nextAssignee}"></userTask>
		<sequenceFlow id="flow8" sourceRef="usertask6" targetRef="usertask7"></sequenceFlow>
		<userTask id="usertask8" name="备件主管审核" activiti:assignee="${nextAssignee}">
			<documentation>备件主管审核最终物料信息</documentation>
			<extensionElements>
				<activiti:taskListener event="create"
					class="com.jojo.service.process.usertask.listener.TaskAssignmentHandler">
					<activiti:field name="approvedRequired">
						<activiti:string><![CDATA[yes]]></activiti:string>
					</activiti:field>
					<activiti:field name="isLastNode">
						<activiti:string><![CDATA[yes]]></activiti:string>
					</activiti:field>
				</activiti:taskListener>
			</extensionElements>
		</userTask>
		<sequenceFlow id="flow12" sourceRef="usertask7" targetRef="usertask8"></sequenceFlow>
		<serviceTask id="servicetask1" name="轮机长提交船舶备件申请" activiti:class="com.jojo.service.process.task.servicetask.DefaultTask4Log"></serviceTask>
		<sequenceFlow id="flow18" sourceRef="servicetask1" targetRef="userTaskGLMgrcheck"></sequenceFlow>
		<exclusiveGateway id="exclusivegateway1" name="Parallel Gateway"></exclusiveGateway>
		<exclusiveGateway id="exclusivegateway2" name="Exclusive Gateway"></exclusiveGateway>
		<sequenceFlow id="flow19" sourceRef="usertask8" targetRef="exclusivegateway2"></sequenceFlow>
		<sequenceFlow id="flow20" name="打回" sourceRef="exclusivegateway2" targetRef="usertask7">
			<conditionExpression xsi:type="tFormalExpression"><![CDATA[${approvedByManager == 2}]]></conditionExpression>
		</sequenceFlow>
		<sequenceFlow id="flow21" name="通过" sourceRef="exclusivegateway2" targetRef="theEnd">
			<conditionExpression xsi:type="tFormalExpression"><![CDATA[${approvedByManager == 1}]]></conditionExpression>
		</sequenceFlow>
		<startEvent id="startevent" name="Start"></startEvent>
		<sequenceFlow id="flow22" sourceRef="startevent" targetRef="servicetask1"></sequenceFlow>
	</process>
